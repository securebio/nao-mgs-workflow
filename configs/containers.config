// Specify Docker containers for workflow processes
process {
    // Seqera containers, built with bin/build_wave_container.py
    withLabel: biopython {
          container = "community.wave.seqera.io/library/pip_biopython_pandas:29325bae61ebabb0"
    }
    withLabel: bracken_plus_utils {
        container = "community.wave.seqera.io/library/bracken_file_gzip:2ad1968722ae663d"
    }
    withLabel: coreutils {
        container = "community.wave.seqera.io/library/coreutils:9.5--ea2e10b8a8e92301"
    }
    withLabel: coreutils_file {
        container = "community.wave.seqera.io/library/coreutils_file:42e98bc9753da1ae"
    }
    withLabel: coreutils_gzip_gawk {
        container = "community.wave.seqera.io/library/coreutils_gzip_gawk:947c12ccae1dc1d7"
    }
    withLabel: curl {
        container = "community.wave.seqera.io/library/curl:8.10.1--9df3b965b4bda7ac"
    }
    withLabel: cutadapt {
        container = "community.wave.seqera.io/library/cutadapt:5.0--991bbd2e184b7014"
    }
    withLabel: ncbi_genome_download {
          container = "community.wave.seqera.io/library/ncbi-genome-download:0.3.3--3034416f2eb9d25d"
    }
    withLabel: pandas {
        container = "community.wave.seqera.io/library/pandas:2.2.3--9b034ee33172d809"
    }
    withLabel: pysam_biopython {
        container = "community.wave.seqera.io/library/pip_biopython_pandas_pysam:b314b57eae1259f3"
    }
    withLabel: python {
        container = "community.wave.seqera.io/library/python:3.13.1--b96bc8a0f20cb0b3"
    }
    withLabel: seqkit {
        container = "community.wave.seqera.io/library/seqkit:2.10.0--03b4774218b4b7ef"
    }
    withLabel: tar_wget {
        container = "community.wave.seqera.io/library/coreutils_tar_wget:7c944a0774f605a8"
    }
    withLabel: unzip {
        container = "community.wave.seqera.io/library/unzip:6.0--692739d3f17122d1"
    }
    withLabel: vsearch {
        container = "community.wave.seqera.io/library/vsearch_gzip:837743cf6c0913f7"
    }
    withLabel: MultiQC {
        // NB: As of 2024-07-01, newer versions currently cause errors
        container = "thatdnaguy/multiqc:v1.21_01"
    }
    withLabel: FASTQC {
        container = "staphb/fastqc:0.12.1"
    }
    withLabel: BBTools {
        // NB: As of 2024-07-01, newer versions currently cause errors
        container = "staphb/bbtools:39.01"
    }
    withLabel: BLAST {
        container = "securebio/nao-blast-awscli:latest"
    }
    withLabel: Kraken2 {
        container = "securebio/nao-kraken2-awscli:latest"
    }
    withLabel: Bracken {
        container = "staphb/bracken:2.9"
    }
    withLabel: tidyverse {
        container = "rocker/tidyverse:4.4.2"
    }
    withLabel: R {
        container = "securebio/nao-rpkg:latest"
    }
    withLabel: seqtk {
        container = "staphb/seqtk:1.4"
    }
    withLabel: fastp {
        container = "staphb/fastp:0.23.4"
    }
    withLabel: filtlong {
        container = "staphb/filtlong:0.2.1"
    }
    withLabel: bowtie2_samtools {
        container = "securebio/nao-bowtie2-samtools-awscli:latest"
    }
    withLabel: minimap2_samtools {
        container = "securebio/nao-minimap2-samtools-awscli:latest"
    }
}
